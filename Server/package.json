{
  "name": "andrew-peterson-website-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/app.js",
    "serve": "nodemon",
    "lint": "npx eslint . --ext .ts,.tsx && prettier \"src/**/*.ts\"",
    "fix": "npx eslint . --ext .ts,.tsx --fix && prettier \"src/**/*.ts\" --write",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "main": "build/app.js",
  "author": "Andrew Peterson",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.3.2",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "morgan": "^1.10.0",
    "pug": "^3.0.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint",
      "prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
